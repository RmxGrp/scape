{
  "manifest_version": 2,
  "name": "Scape",
  "description": "Give your browser the fresh coat of paint it deserves!",
  "version": "2.1.2",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "128": "128.png",
    "48": "48.png",
    "16": "16.png"
  },
  "options_ui": {
    "open_in_tab": true,
    "page": "options.html"
  },
  "permissions": ["storage", "geolocation"]
}
